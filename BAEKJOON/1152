package JavaStudy;

import java.util.Scanner;
import java.util.StringTokenizer;

class Main {
    public static void main(String[] args) {
    	Scanner sc = new Scanner(System.in);
    	String input = sc.nextLine();
    	StringTokenizer st = new StringTokenizer(input, " "); //Tokenizer를 사용해서 입력받은 input의 " "를 구분

    	System.out.println(st.countTokens()); //구분한 문장의 단어 개수를 출력
    }
}
